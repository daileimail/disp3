{
  "databases": {
		"$list": {
			"type": "mongo",
			"host": {"$type":"string"},
			"port": {"$type":"number"},
			"username": {"$type":"string"},
			"password": {"$type":"string"},
			"database": "test",
			"withSchemas": {
				"$multi": true,
				"$type": "string",
				"$from": "proto.schemas"
			}
		}
  },
  "servers": {
		"$list": {
      "type": "nodejs",			
			"ip": {"$type": "string"},
			"port": {"$type": "number"},
			"protocol": "http",
			"static": {
				"$type": "string"
			},
			"log": {"$type":"number"},
			"withApis": {
				"$multi": true,
				"$type": "string",
				"$from": "proto.apis"
			},
			"withDatabases": {
				"$multi": true,
				"$type": "string",
				"$from": "impl.databases"
			}
		}
	},
  "clients": {
		"$list": {
			"type": "html",
			"withUis": {
				"$multi": true,
				"$type": "string",
				"$from": "proto.uis"
			},
			"withServers": {
				"$multi": true,
				"$type": "string",
				"$from": "impl.servers"
			}
    }
  }
}

