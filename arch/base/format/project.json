{
    "kit": "mono",
    "format": {
				"deps": {
						"type": "list"
				},
				"arch": {
						"type": "string",
						"default": "base"
				},
        "target": {
						"type": "string",
						"default": "."
				},
				"navpaths": {
						"type": "array"
				},
				"navspaces": {
						"type": "array"
				},
        "fsconfigs": {
            "kit": "list",
            "default": {
                ".git": {
                    "ignore": true
                },
                "node_modules": {
                    "ignore": true
                },
                "dispmod": {
                    "ignore": true
                },
								"bower_components": {
										"ignore": true
								}
            }
        }
    }
}
