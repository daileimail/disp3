{
	"deps": {
		"$list": {"$type": "boolean"}
	},
	"arch": "base",
  "target": ".",
	"navpaths": {
		"$type": "string",
		"$multi": 1
	},
	"navspaces": {
		"$type": "string",
		"$multi": 1,
		"$default": ["*.*.*"]
	},
	"posts": {
		"$type": "string",
		"$multi": 1
	},
	"roles": {
		"$list": {
			"$type": "string",
			"$multi": true
		}
	},
  "fsconfigs": {
    "$list":  {
			"ignore": {"$type": "boolean"}
		},
		"$default": {
      ".git": {
        "ignore": true
      },
      "node_modules": {
        "ignore": true
      },
			"bower_components": {
				"ignore": true
			},
			"kits": {
				"ignore": true
			},
			"kit": {
				"ignore": true
			},
			"x": {
				"ignore": true
			}
    }
  }
}
