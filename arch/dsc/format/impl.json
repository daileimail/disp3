{
    "kit": "mono",
    "format": {
        "databases": {
            "kit": "list",
            "format": {
                "type": {
                    "type": "enum",
                    "sets": [
                        "mongo",
                        "mysql"
                    ],
										"default": "mongo"
                },
                "host": "string",
                "port": "number",
                "username": "string",
                "password": "string",
                "database": {
										"type": "string",
										"default": "test"
								},
								"withSchemas": {
										"type": "enums",
										"from": "proto.schemas"
								}
            }
        },
        "servers": {
            "kit": "list",
            "format": {
                "type": {
                    "type": "enum",
                    "sets": [
                        "expressjs",
                        "nodejs",
												"none"
                    ],
										"default": "nodejs"
                },
                "ip": "string",
                "port": "number",
                "protocol": {
										"type": "enums",
										"sets": ["http", "https"],
										"default": "http"
								},
								"static": {
										"type": "string"
								},
								"log": "number",
								"withApis": {
										"type": "enums",
										"from": "proto.apis",
										"check": {
												"in": {
														"auth": "impl.server.withAuths"
												}
										}
								},
								"withDatabases": {
										"type": "enums",
										"from": "impl.databases"
								},
								"withClients": {
										"type": "enums",
										"from": "impl.clients"
								}
            }
        },
        "clients": {
            "kit": "list",
            "format": {
                "type": {
                    "type": "enum",
                    "sets": [
												"html",
                        "android",
                        "ios"
                    ],
										"default": "html"
                },
								"withUis": {
										"type": "enums",
										"from": "proto.uis"
								}
            }
        }
    }
}
