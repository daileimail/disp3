^^function makeCronStr(config){
					 var str = "";
	if(config.text) str += "#" + config.text + "\n";
	if(!config.min) config.min = "*";
	if(!config.hour) config.hour = "*";
	if(!config.day) config.day = "*";
	str += config.min + " " + config.hour + " " + config.day + " * * nohup node ^^=$.pwd$$/cron/" + config.name + ".js";
	return str;
}$$